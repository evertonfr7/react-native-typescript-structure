{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true
  },
  "typeRoots": [
    "./node_modules/@types",
    "./src/@types"
  ],
  "paths": {
    "@components/*": ["./src/components/*"],
    "@contexts/*": ["./src/contexts/*"],
    "@routes/*": ["./src/routes/*"],
    "@services/*": ["./src/services/*"],
    "@views/*": ["./src/views/*"]
  }
}
